{
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/icon-*.png",
            "/manifest.json",
            "/sw.js",
            "/_framework/*",
            "/static/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "text/javascript",
        ".css": "text/css",
        ".png": "image/png"
    },
    "globalHeaders": {
        "Cache-Control": "public, max-age=31536000, immutable"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "platform": {
        "apiRuntime": "node:18"
    }
}